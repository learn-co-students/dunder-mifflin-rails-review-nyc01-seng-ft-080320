<h1>Employee</h1>
<table borders="left">
    <tr>
        <td rowspan="7"><%= image_tag(@employee.img_url)%><td>
    </tr>
    <tr>
        <td><h3>First Name:</h3></td>
        <td><h3><%= @employee.first_name %></h3><td>
    </tr>
    <tr>
        <td><h3>Last Name: </h3></td>
        <td><h3><%= @employee.last_name %></h3></td>
    </tr>
    <tr>
        <td><h3>Alias:</h3></td>
        <td><h3><%= @employee.alias %></h3></td>
    </tr>
    <tr>
        <td><h3>Title:</h3></td>
        <td><h3><%= @employee.title %></h3></td>
    </tr>
    <tr>
        <td><h3>Office:</h3></td>
        <td><h3><%= @employee.office %></h3></td>
    </tr>
    <tr>
        <td><h3>Dog Name:</h3></td>
        <td><h3><%= link_to @employee.dog.name, dog_path(@employee.dog) %></h3></td>
    </tr>
</table>