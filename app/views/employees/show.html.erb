<h1> Employee Show Page</h1>

<p> Name: <%= @employee.first_name + " " + @employee.last_name %> </p>
<p> Alias: <%= @employee.alias %> </p>
<p> Title: <%= @employee.title %> </p>
<p> Title: <%= @employee.office %> </p>
<p> Dog: <%= @employee.dog.name %> </p>
<img src = <%= @employee.img_url %> alt = "image of employee"> <br>


<%= link_to "Index Page", employees_path %> | <%= link_to "Edit Page", edit_employee_path(@employee) %> <br>
<br>
<%= button_to "Delete Employee", employee_path(@employee), method: :delete %>